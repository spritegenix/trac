<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /

    # If request includes /home/gettrac/public_html/ â†’ redirect to clean path
    RewriteCond %{REQUEST_URI} ^/home/gettrac/public_html/(.*)$ [NC]
    RewriteRule ^home/gettrac/public_html/(.*)$ /$1 [L,R=301]

    # Remove .html from URLs
    RewriteCond %{THE_REQUEST} \s/([^.]+)\.html[\s?] [NC]
    RewriteRule ^ %1 [L,R=301]

    # Internally serve .html if no extension given
    RewriteCond %{REQUEST_FILENAME}.html -f
    RewriteRule ^ %{REQUEST_URI}.html [L]
</IfModule>